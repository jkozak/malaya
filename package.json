{
  "name": "malaya",
  "version": "0.8.991",
  "description": "Logic programming with JSON pattern matching",
  "dependencies": {
    "argparse": "^2.0.1",
    "browserify": "^17.0.0",
    "byline": "^5.0.0",
    "chalk": "^4.1.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.3",
    "digest-stream": "^2.0.0",
    "esprima": "=1.2.2",
    "express": "^4.17.1",
    "express-minify": "^1.0.0",
    "express-ws": "^4.0.0",
    "ip": "^1.1.4",
    "js-yaml": "^4.1.0",
    "jsdom": "^9.11.0",
    "json5": "^2.2.0",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "monotonic-timestamp": "0.0.9",
    "morgan": "^1.10.0",
    "multistream": "^4.1.0",
    "random-js": "^1.0.8",
    "reactify": "^1.1.1",
    "recast": "=0.9.5",
    "rimraf": "^3.0.2",
    "safe-eval": "^0.4.1",
    "shelljs": "^0.8.4",
    "stream-to-array": "^2.3.0",
    "temp": "^0.9.4",
    "through2": "^4.0.2",
    "underscore": "^1.13.1",
    "verror": "^1.6.0",
    "websocket": "^1.0.34",
    "websocket-stream": "^5.5.2",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "connect": "^3.7.0",
    "depcheck": "^1.4.1",
    "eslint": "^7.26.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-security": "^1.4.0",
    "immutable": "^4.0.0-rc.12",
    "jsverify": "^0.8.4",
    "matcha": "^0.7.0",
    "mocha": "^8.4.0",
    "node-jsx": "^0.13.3",
    "prettyjson": "^1.0.0",
    "request": "^2.88.2",
    "resumer": "0.0.0",
    "sinon": "^10.0.0",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "rules",
    "json",
    "logic",
    "CHR",
    "pattern",
    "matching",
    "prevalence"
  ],
  "license": "AGPL-1.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkozak/malaya.git"
  },
  "scripts": {
    "postinstall": "node for-sub-npm-modules.js \"npm install\" plugins",
    "benchmark": "matcha -R plain benchmark/*.js",
    "eslint": "eslint -f compact .",
    "pretest": "npm run eslint --silent",
    "test:js": "cross-env NODE_ENV=test mocha --exit -C test/*.js plugins/test/*.js examples/test/*.js",
    "test:chrjs": "cross-env NODE_ENV=test mocha --require compiler -C test/*.chrjs",
    "test": "npm run test:js --silent && npm run test:chrjs --silent",
    "test:chrjs:XXX": "cross-env NODE_ENV=test mocha --require compiler -C test/*.chrjs --grep XXX",
    "test:XXX": "cross-env NODE_ENV=test mocha --exit --require compiler -C test plugins/test examples/test --grep XXX",
    "init": "node malaya init",
    "start": "node malaya run"
  },
  "main": "index.js",
  "bin": [
    "malaya",
    "client.js",
    "dump.js",
    "repl.js"
  ]
}
