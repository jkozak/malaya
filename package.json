  "scripts": {
    "benchmark": "matcha -R plain benchmark/*.js",
    "eslint": "eslint -f compact .",
    "pretest": "npm run eslint --silent",
    "test:js": "cross-env NODE_ENV=test mocha --exit -C test/*.js plugins/test/*.js examples/test/*.js",
    "test:malaya": "cross-env NODE_ENV=test mocha --require compiler -C test/*.malaya",
    "test": "npm run test:js --silent && npm run test:malaya --silent",
    "test:malaya:XXX": "cross-env NODE_ENV=test mocha --require compiler -C test/*.malaya --grep XXX",
    "test:XXX": "cross-env NODE_ENV=test mocha --exit --require compiler -C test plugins/test examples/test --grep XXX",
    "init": "node malaya init",
    "start": "node malaya run"
  },
  "main": "index.js",
  "bin": [
    "malaya",
    "client.js",
    "dump.js",
    "repl.js"
