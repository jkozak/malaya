{
    "bin": [
        "malaya",
        "client.js",
        "dump.js",
        "repl.js"
    ],
    "config": {
        "ssh": "malaya.site"
    },
    "main": "index.js",
    "scripts": {
        "buildHtmlBook": "emacs book.org --batch -f org-html-export-to-html --kill",
        "buildHtmlFAQ": "emacs FAQ.org --batch -f org-html-export-to-html --kill",
        "buildHtmlDocs": "npm run buildHtmlFAQ && npm run buildHtmlBook",
        "deploy": "echo \"cd /malaya.git && git push -f deploy new-compiler:master && git tag DEPLOY--$(date +%Y-%m-%d_%H.%M.%S)\" | ssh -AT $npm_package_config_ssh sh",
        "prestart": "[ -d .prevalence ] || npm run init",
        "start": "node malaya run -D",
        "init": "node malaya init",
        "install": "npm run buildHtmlDocs",
        "test:XXX": "cross-env NODE_ENV=test mocha --exit --require compiler -C test plugins/test examples/test --grep XXX",
        "test": "npm run test:js --silent && npm run test:malaya --silent",
        "test:malaya": "cross-env NODE_ENV=test mocha -R min --require compiler -C test/*.malaya",
        "test:js": "cross-env NODE_ENV=test mocha --exit -R min -C test/*.js plugins/test/*.js examples/test/*.js",
        "pretest": "npm run eslint --silent",
        "eslint": "eslint -f compact .",
        "benchmark": "matcha -R plain benchmark/*.js",
        "postinstall": "node for-sub-npm-modules.js \"npm install\" plugins"
    },
    "repository": {
        "url": "git+https://github.com/jkozak/malaya.git",
        "type": "git"
    },
    "private": false,
    "license": "AGPL-1.0",
    "keywords": [
        "rules",
        "json",
        "logic",
        "CHR",
        "pattern",
        "matching",
        "prevalence"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "engineStrict": true,
    "devDependencies": {
        "supertest": "^3.4.2",
        "superagent": "^4.1.0",
        "sinon": "^1.17.7",
        "resumer": "0.0.0",
        "request": "^2.88.0",
        "prettyjson": "^1.0.0",
        "node-jsx": "^0.13.3",
        "mocha": "^5.2.0",
        "matcha": "^0.7.0",
        "jsverify": "^0.7.4",
        "immutable": "^4.0.0-rc.12",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-react": "^7.12.4",
        "eslint-plugin-node": "^8.0.1",
        "eslint-plugin-mocha": "^5.2.1",
        "eslint-plugin-chai-expect": "^2.0.1",
        "eslint": "^5.13.0",
        "depcheck": "^0.7.1",
        "cross-env": "^3.1.4",
        "chai": "^4.2.0"
    },
    "dependencies": {
        "argparse": "^1.0.10",
        "basic-auth": "^1.0.1",
        "browserify": "^14.5.0",
        "byline": "^5.0.0",
        "chalk": "^1.1.3",
        "cookie-parser": "^1.4.3",
        "digest-stream": "^2.0.0",
        "esprima": "=1.2.2",
        "express": "^4.16.4",
        "express-minify": "^0.2.0",
        "express-ws": "^4.0.0",
        "ip": "^1.1.4",
        "jmespath": "^0.15.0",
        "js-yaml": "^3.13.1",
        "jsdom": "^9.11.0",
        "json5": "^2.1.0",
        "minimist": "^1.1.1",
        "moment": "^2.24.0",
        "monotonic-timestamp": "0.0.9",
        "morgan": "^1.9.1",
        "multistream": "^2.1.1",
        "pegjs": "^0.10.0",
        "random-js": "^1.0.8",
        "reactify": "^1.1.1",
        "recast": "=0.9.5",
        "rimraf": "^2.6.3",
        "shelljs": "^0.7.6",
        "stream-to-array": "^2.3.0",
        "temp": "^0.8.0",
        "through2": "^2.0.5",
        "underscore": "^1.9.1",
        "verror": "^1.6.0",
        "websocket": "^1.0.28",
        "websocket-stream": "^5.1.2",
        "ws": "^6.1.3"
    },
    "description": "Logic programming with JSON pattern matching",
    "version": "0.8.99",
    "name": "malaya"
}
