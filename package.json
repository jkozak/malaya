{
  "name": "malaya",
  "version": "0.8.99",
  "description": "Logic programming with JSON pattern matching",
  "dependencies": {
    "agly": "0.0.1",
    "argparse": "^1.0.2",
    "basic-auth": "^1.0.1",
    "browserify": "^14.1.0",
    "byline": "^5.0.0",
    "chalk": "^1.1.3",
    "cookie-parser": "^1.4.3",
    "digest-stream": "^2.0.0",
    "esprima": "=1.2.2",
    "express": "^4.14.1",
    "express-minify": "^0.2.0",
    "express-ws": "^4.0.0",
    "ip": "^1.1.4",
    "jmespath": "^0.15.0",
    "js-yaml": "^3.12.0",
    "jsdom": "^9.11.0",
    "json5": "^2.1.0",
    "minimist": "^1.1.1",
    "moment": "^2.22.2",
    "monotonic-timestamp": "0.0.9",
    "morgan": "^1.8.1",
    "multistream": "^2.1.0",
    "node-sockjs-client": "^1.0.7",
    "random-js": "^1.0.8",
    "reactify": "^1.1.1",
    "recast": "=0.9.5",
    "rimraf": "^2.6.0",
    "shelljs": "^0.7.6",
    "stream-to-array": "^2.3.0",
    "temp": "^0.8.0",
    "through2": "^2.0.3",
    "underscore": "^1.9.1",
    "verror": "^1.6.0",
    "websocket": "^1.0.28",
    "websocket-stream": "^5.1.2",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^3.1.4",
    "eslint": "^5.6.0",
    "eslint-plugin-chai-expect": "^2.0.1",
    "eslint-plugin-mocha": "^5.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-security": "^1.4.0",
    "immutable": "^4.0.0-rc.12",
    "jsverify": "^0.7.4",
    "matcha": "^0.7.0",
    "mocha": "^5.2.0",
    "node-jsx": "^0.13.3",
    "prettyjson": "^1.0.0",
    "request": "^2.88.0",
    "resumer": "0.0.0",
    "sinon": "^1.17.7",
    "supertest": "^3.3.0"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "rules",
    "json",
    "logic",
    "CHR",
    "pattern",
    "matching",
    "prevalence"
  ],
  "license": "AGPL-1.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkozak/malaya.git"
  },
  "scripts": {
    "benchmark": "matcha -R plain benchmark/*.js",
    "eslint": "eslint -f compact .",
    "pretest": "npm run eslint --silent",
    "test:js": "cross-env NODE_ENV=test mocha --exit -R min -C test/*.js test/plugins/*.js",
    "test:js:crap": "cross-env NODE_ENV=test mocha --exit -R min -C test/compiler.js test/engine.js test/plugin.js",
    "test:chrjs": "cross-env NODE_ENV=test mocha --exit -R min --require compiler -C test/*.chrjs",
    "test": "npm run test:js --silent && npm run test:chrjs --silent",
    "test:XXX": "cross-env NODE_ENV=test mocha --exit -R min --require compiler -C test test/plugins --grep XXX",
    "init": "node malaya init",
    "start": "node malaya run"
  },
  "main": "index.js",
  "bin": [
    "malaya",
    "client.js",
    "dump.js",
    "repl.js"
  ]
}
