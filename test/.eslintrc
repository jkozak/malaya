// -*- mode: json -*-
{
    "env": {
        "mocha": true
    },
    "plugins": ["mocha","chai-expect"],
    "rules": {
        "no-unused-vars":1,
        "no-constant-condition":1,
        "mocha/no-exclusive-tests": "error",
        "chai-expect/missing-assertion": 2,
        "chai-expect/terminating-properties": 1,
        "security/detect-eval-with-expression": 0
    }
}
