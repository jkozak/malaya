// -*- mode: json -*-
{
    "env": {
        "mocha": true
    },
    "plugins": ["mocha"],
    "rules": {
        "no-unused-vars":1,
        "no-constant-condition":1,
        "mocha/no-exclusive-tests": "error",
        "security/detect-eval-with-expression": 0,
        "node/no-unsupported-features/node-builtins": ["error",{ignores:['assert.strict']}]
    }
}
