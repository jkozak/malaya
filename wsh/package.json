{
  "name": "wsh",
  "version": "0.0.1",
  "description": "minimal shell for using make with node under windows",
  "main": "wsh.js",
  "scripts": {
    "eslint": "eslint -c test/eslint.conf -f compact --env node *.js",
    "eslint:test":  "eslint -c test/eslint.conf -f compact --env mocha test/*.js",
    "pretest": "npm run eslint && npm run eslint:test",
    "test": "env NODE_ENV=test mocha -R mocha-tldr-reporter test"
  },
  "dependencies": {
    "glob": "^5.0.14",
    "rimraf": "^2.4.3",
    "verror": "^1.6.0"
  },
  "devDependencies": {
    "eslint": "^1.4.3",
    "mocha": "^2.1.0",
    "mocha-tldr-reporter": "^1.0.0",
    "temp": "^0.8.3"
  },
  "engines": [
    "node >=0.10.0"
  ],
  "keywords": [
    "windows",
    "shell",
    "make"
  ],
  "author": "John Kozak <jk@malaya.club>",
  "license": "MIT",
  "repository": {
    "type": "hg",
    "url": "ssh://malaya.club//"
  },
  "bin":{
    "wsh":"./wsh.cmd",
    "env":"./wsh.cmd"
  },
  "main": "wsh.js"
}
